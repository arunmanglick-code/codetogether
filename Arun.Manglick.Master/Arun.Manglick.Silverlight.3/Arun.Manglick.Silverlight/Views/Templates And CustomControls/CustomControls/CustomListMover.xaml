             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <local:ListMover LeftListBoxHeader="Arun" RightListBoxHeader="World"></local:ListMover>
        </StackPanel>
    </Grid>
</UserControl>
