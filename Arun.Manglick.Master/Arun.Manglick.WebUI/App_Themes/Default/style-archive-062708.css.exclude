/* -------- Pagination element -------- */
.results-returned
{
	font-family: Arial, sans-serif;
	font-weight: normal;
	text-decoration: none;
	color: #102162;
	/*display: inline;*/
	font-size: 9px;
}
.results-pagination
{
	text-align: right;
	font-family: Arial, sans-serif;
	font-weight: normal;
	text-decoration: none;
	color: #102162;
	background-color: #e8e8e8;
	padding: 0;
	border-width: 1px;
	border-style: solid;
	border-color: #dddddd;
	margin: 0;
	/*display: inline;*/
	font-size: 9px;
}


.results-pagination a
{
	font-family: Arial, sans-serif;
	font-size: 9px;
	background-color: #e8e8e8;
	text-decoration: underline;
	font-weight: normal;
}

.results-pagination a:visited
{
	color: #666666;
	background-color: #e8e8e8;
	text-decoration: underline;
}
.results-pagination a:hover
{
	color: #ff0000;
	background-color: #e8e8e8;
	text-decoration: underline;	
}
.results-pagination a:link
{
	color: #102162;
	background-color: #e8e8e8;
	text-decoration: underline;
}

.results-pagination a:active
{
	color: #ff0000;
	background-color: #e8e8e8;
	text-decoration: underline;
}
