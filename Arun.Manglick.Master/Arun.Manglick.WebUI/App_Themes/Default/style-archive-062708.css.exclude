body
{
	padding: 0px;
	margin: 0px;
	font-size: 10px;
}

a
{
	text-decoration: underline;
}
a:link
{
	color: #284E98;
	text-decoration: none;
}
a:visited
{
	color: #284E98;
	text-decoration: none;
}
a:hover
{
	color: #284E98;
	text-decoration: underline;
}
a:active
{
	color: #284E98;
	text-decoration: none;
	font-weight: bold;
}

#tdHeaderLinks a
{
	font-family: Verdana, Arial;
	font-weight:normal;
	font-size:7pt;
	text-decoration:underline;
}

table
{
	padding: 0px;
	margin: 0px;
	font-size: 10px;
}

#tblObjectDetails td {
    padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 2px;
	padding-right: 2px; }

#tblObjectDetails th
{
	padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 2px;
	padding-right: 2px;
	vertical-align:middle;
	font: 10px Verdana, Arial, Helvetica, sans-serif;
    font-size:10px;
    font-weight:bold;
    background-color:#E1E9EC;
}

/*--Master page framework -----------------------------*/
.divmaster
{
	padding: 0px;
	margin: 0px;
	width: 100%;
}
.tablemaster
{
	padding: 0px;
	margin: 0px;
	width: 100%;
	border: 0px;
}

/*--Header -----------------------------*/
.appheader
{
	padding: 5px;
	font-weight: bold;
	margin: 0px;
	vertical-align: middle;
	overflow: hidden;
	width: 100%;
	color: #999999;
	font-family: Arial, Verdana;
	height: 40px;
	background: #F2F2F2;
	text-align: left;
	border-bottom: 1px solid #999999;
}

.appheadernoborder
{
	padding: 5px;
	font-weight: bold;
	margin: 0px;
	vertical-align: middle;
	overflow: hidden;
	width: 100%;
	color: #999999;
	font-family: Arial, Verdana;
	height: 40px;
	background: #F2F2F2;
	text-align: left;
}

/*--Footer -----------------------------*/
.footer
{
	border-top: 3px solid #f7f7f7;
	padding: 0px;
	margin: 0px;
	vertical-align: middle;
	font-family: Verdana, Arial;
	font-size: 9px;
	height: 40px;
	background-color: #ffffff;
	text-align: center;
	border-top: 2px solid #99BDCB;
}


/*--Left navigation space and tree navigation-----------------------------*/
.leftnavigation
{
	border-right: 2px solid #99BDCB;
	background: #ffffff;
	padding-top: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	padding-right: 4px;
	margin: 0px;
	width: 175px;
	vertical-align: top;
}

.TreeNavigation
{
	font-size: 9pt;
	font-family: Arial, Verdana, Sans-Serif;
	left: 0px;
	vertical-align: top;
	color: #999999;
	position: relative; /*top: 2px; */
	background-color: transparent;
	text-align: left;
	width: 175px; /*line-height: 8pt;*/
}
