	position: relative;
}

.GridHeader1
{
	background-color: #CAD2D4;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9pt;
	color: #000000;
	font-weight: bold;
	text-align: center;
	line-height: 18px;
	border-bottom: solid 1px #FFFFFF;
	/*position: relative;*/
}

.GridHeader a, .GridHeader a:active, .GridHeader a:visited
{
	text-decoration: none;
	color: #000000;
}

.GridHeader a:hover
{
	text-decoration: underline;
}

.GridItem, .GridAltItem
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 8pt;
	color: #000000;
	font-weight: normal;
	padding: 3px 3px 3px 3px;
	border: solid 1px #FFFFFF;
}
.GridItem
{
	background-color: #EFEFEF;
}
.GridAltItem
{
	background-color: #F7F7F7;
}
.GridItem a
{
	text-decoration: underline;
	color: #456AAE;
}

.GridItem a:hover, .GridItem a:active, .GridItem a:visited
{
	text-decoration: underline;
}
.GridAltItem a
{
	text-decoration: underline;
	color: #456AAE;
}

.GridAltItem a:, .GridAltItem a:active, .GridAltItem a:visited
{
	text-decoration: underline;
}
.GridPager
{
	font-family: Arial, Helvetica, sans-serif;
