.TreeNavigation a:link {color:#000033;}
.TreeNavigation a:visited {color:#000099;}
.TreeNavigation a:hover {color:Red}
.TreeNavigation a:active {color:Green;}*/
.SelectedNode
{
	font-weight: bold;
}
.RootNode
{
	font-family: Arial;
	font-size: 9pt; /*line-height: 8pt;*/
}
.ParentNode
{
	font-family: Arial;
	font-size: 9pt; /*line-height: 8pt;*/
}
.HoverNode
{
}
.LeafNode
{
	font-family: Arial;
	font-size: 9pt; /*line-height: 8pt;*/
}
.Node
{
	font-family: Arial;
	font-size: 9pt; /*line-height: 8pt;*/
}

/*--Workspace -----------------------------*/
.workspace
{
	padding: 0px;
	margin: 0px;
	width: 100%;
	background-color: white;
	vertical-align: top;
}

/*--Breadcrumb trail navigation in the workspace -----------------------------*/
.breadcrumb
{
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	background: #F2F2F2;
	height: 25px;
	padding-left: 8px;
	border-bottom: solid 2px #E4E8EA;
}
/*.breadcrumb  a { }
.breadcrumb  a:link {color:#000033;}
.breadcrumb  a:visited {color:#000099;}
.breadcrumb  a:hover {color:Red}
.breadcrumb  a:active {color:Green;}*/

.backbutton /*cell for back button formatting with class=button */
{
	background: #F2F2F2;
	height: 25px;
	border-bottom: solid 2px #E4E8EA;
}

.mdelogo
{
	color: #CB8E34;
	font-weight: bold;
	font-family: Helvetica, Arial, sans-serif;
	font-size: 20px;
	height: 25px;
}

/*---Sits above task space - this is the name of the active task ----*/
.title
{
	color: #CB8E34;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	border-bottom: solid 1px #E5A335;
	height: 25px;
	font-style: italic;
}

.titleInner
{
	color: Green;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	height: 25px;
	font-style: italic;
}

.titlelogin
{
	color: #CB8E34;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 15px;
	border-bottom: solid 2px #E5A335;
	height: 27px;
	font-style: italic;
}

.titleloginnew
{
	color: #000000;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 15px;
	height: 27px;
	font-style: normal;
}


/*-- used for NEGATIVE validation messages--*/
.validation-error
{
	padding-right: 4px;
	padding-left: 4px;
	font-size: 8pt;
	padding-bottom: 1px;
	vertical-align: middle;
	color: red;
	padding-top: 1px;
	font-family: Arial;
	list-style-type: none;
	background-color: white;
	text-align: left;
}

.validation-error-select
{
	padding-right: 4px;
	padding-left: 4px;
	font-size: 8pt;
	padding-bottom: 1px;
	vertical-align: middle;
	color: red;
	padding-top: 1px;
	font-family: Arial;
	list-style-type: none;
	background-color: white;
	text-align: left;
	display:none;
}

/*-- used for POSITIVE validation messages--*/
.validation-no-error
{
	padding-right: 4px;
	padding-left: 4px;
	font-size: 8pt;
	padding-bottom: 1px;
	vertical-align: middle;
	color: Navy;
	padding-top: 1px;
	font-family: Arial;
	list-style-type: none;
	background-color: white;
	text-align: left;
}

/*--Holds the tabs------------*/ 
.tabtable {
	padding-left: 0px; 
	padding-right: 0px; 
	padding-top: 0px; 
	padding-bottom: 0px; 
	margin: 0px;
	/*background-color:#F7F7F7;*/
	background-color:#FFFFFF;
	/*width:100%; */}

.tabtable-selected-tab {
   padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 10px;
	padding-right: 10px;
	vertical-align:middle;
	text-align: center;
	background-color:#FFFFFF;
	font: 10px Verdana, Arial, Helvetica, sans-serif;
    font-weight:bold;
    width: 125px;
    border-top: 1px solid #00529b; 
    border-right: 1px solid #00529b; 
    border-left: 1px solid #00529b; }
 
.tabtable-unselected-tab {
   padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 10px;
	padding-right: 10px;
	vertical-align:middle;
	text-align: center;
	background-color:#00529b;
	color: #ffffff;
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	width: 125px; }   

/*--Forms within the workspace -----------------------------*/
/*--Holds the table that formats the form------------*/
#divForm
{
	padding: 2px;
	font-family: Arial, Verdana, Sans-Serif; /*font-size:10pt;*/
	line-height: 1em;
}

/*--Holds the entire interaction element from primary form to button row------------*/
#divtable
{
	padding: 0px;
	margin: 0px;
	border:solid 0px blue;
}
#divtable td
{
	padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 2px;
	padding-right: 2px;
	vertical-align: middle;
	border:solid 0px red;
}
#divtable td #buttonTable td, #divtable td #buttonTable2 td
{
	padding: 0px;
	border:solid 0px green;
}


/*--Holds the primary form------------*/
#formtable
{
	width: 100%;
	padding: 0px;
	margin: 0px; /*background-color:#F7F7F7;*/
	background-color: #FFFFFF;
}
#formtable td
{
	padding-top: 2px;
	padding-bottom: 3px;
	padding-left: 2px;
	padding-right: 2px;
	vertical-align: middle;
}
#headertable
{
	width: 510px;
	padding: 0px;
	margin: 0px; 
	background-color: #FFFFFF;
}
/*--TASK Header------------*/
.requiredfieldmessage
{
	/* *Required Fields */
	text-align: left;
	font-family: inherit;
	font-size: 9px;
	font-style: italic;
	line-height: inherit;
}

.asterisk
{
	font-family: inherit;
	font-size: 9px;
	color: red;
}

.formheader
{
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-weight: bold;
	background-color: #E1E9EC;
	line-height: 18px;
	padding-left: 5px;
}

/*--THIS IS THE ASTERISK FORMATTING FOR REQUIRED FIELDS------------*/
.requiredcolumn
{
	text-align: right;
	width: 10px;
	font-family: Verdana, Arial;
	font-size: 9px;
	color: red;
}

/*--Label tds------------*/
.labelcolumn
{
	text-align: left;
	font-family: Verdana, Arial;
	font-size: 9px;
	line-height: .95em;
}

.labelterms
{
	text-align: left;
	font-family: Verdana, Arial;
	font-size: 11px;
}

.labellogin
{
	text-align: left;
	font-family: Verdana, Arial;
	font-size: 12px;
}

.labelcolumnbold
{
	text-align: left;
	font-family: Verdana, Arial;
	font-size: 9px;
	font-weight: bold;
	line-height: .95em;
}
/*--Input field tds------------*/
.inputcolumn
{
	text-align: left;
	font-family: Arial;
	font-size: 9px;
}

.inputcolumnRight
{
	text-align: right;
	font-family: Arial;
	font-size: 9px;
}
/*--Input fields, all types------------*/
.inputfield
{
	font-size: 10px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	background: #F2F2F2;
	border: solid 1px #88B0CA;
}


/*--review versions for text only views, no input fields used --*/
.inputcolumnreview
{
	font-family: Arial, Verdana, Sans-Serif;
	font-size: 9px;
	font-weight: bold;
}
.labelcolumnreview
{
	text-align: left;
	font-family: Verdana, Arial;
	font-size: 10px;
	line-height: .95em;
	border-left: 1px solid #999999;
}

