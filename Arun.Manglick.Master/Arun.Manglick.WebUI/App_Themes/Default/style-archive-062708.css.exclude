/* for buttons at bottom of screen(task-level buttons) */
.buttoncell
{
	/*this is applied to the cell holding the buttontable*/ /*text-align: left;     background-color: #e8e8e8;*/
}
.buttontable, .buttontable2
{
	/*three-cell table holding the button*/ /*-CLOSE- -HISTORY- -RESET-  |  -DELETE- -NEW-   | -SAVE- or -SEARCH- */
	width: 100%;
	padding: 0px;
	margin: 0px;
}

.button
{
	/* DO NOT APPLY WIDTHS TO BUTTONS */
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	background: #B5C2C7;
	border: solid 1px #70868F;
	color: #000000;
	font-weight: bold;
}

.buttonlogin
{
	/* DO NOT APPLY WIDTHS TO BUTTONS */
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	background: #000099;
	border: solid 1px #70868F;
	color: #CCFFFF;
	font-weight: bold;
}

.button[disabled]
{
	/* DO NOT APPLY WIDTHS TO BUTTONS */
	font: 10px Verdana, Arial, Helvetica, sans-serif;
	background-color: #CFDEE4;
	border: solid 1px #70868F;
	color: #ACA899;
	font-weight: bold;
}

.Splitter
{
	cursor: e-resize;
	width: 2px;
	background-color: Gray;
	height: 100%;
}
.FullScreen
{
	height: 600px;
}

