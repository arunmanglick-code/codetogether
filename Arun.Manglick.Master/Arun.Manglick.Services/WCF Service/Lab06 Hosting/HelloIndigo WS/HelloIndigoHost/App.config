<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="HIBehavior">
          <serviceMetadata  />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="HIBehavior" name="HelloIndigo.Lab6.WS.HelloIndigoService">
        <clear />
        <endpoint address="HelloIndigoService"
                 binding="wsHttpBinding" name="wsHttp"
                 contract="HelloIndigo.Lab6.WS.IHelloIndigoService" />
        
        <endpoint address="HelloIndigoService"
                 binding="netTcpBinding" name="netTcp"
                 contract="HelloIndigo.Lab6.WS.IHelloIndigoService" />

        <endpoint binding="mexHttpBinding" name="mex" contract="IMetadataExchange" />
        <endpoint binding="mexTcpBinding" name="mex" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8000/HelloIndigoWS" />
            <add baseAddress="net.tcp://localhost:9100/HelloIndigoWS"/>
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>