<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="HIBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="HIBehavior" name="HelloIndigo.Lab6.WIN.HelloIndigoService">
        <clear />
        <endpoint address="HelloIndigoService"
                 binding="netTcpBinding" name="netTcp"
                 contract="HelloIndigo.Lab6.WIN.IHelloIndigoService" />

        <endpoint binding="mexTcpBinding" name="mex" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:9001/HelloIndigoWIN"/>
            <add baseAddress="http://localhost:8000/HelloIndigoWIN" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>