    <Grid x:Name="LayoutRoot"
          Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <local:CountySearchViewModelConnector x:Name="countySearchVMC" />
        <AppViewState:ApplicationContextViewStateConnector x:Name="ApplicationContext"></AppViewState:ApplicationContextViewStateConnector>
        <TextBlock Text="Name Search Results"
                   Style="{StaticResource PageHeaderText}"
                   Grid.Row="0"></TextBlock>
        <Grid Grid.Row="1"
              Margin="10,0,10,0"
              x:Name="grdHeaderInfo"
              DataContext="{Binding Path=NameResultBindableModel, ElementName=countySearchVMC, Mode=TwoWay}">
            <Grid.Resources>
                <local:EmptyStringToVisibility x:Name="EmptyStringToVisibility"></local:EmptyStringToVisibility>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition MinHeight="50" />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition Height="50" />
            </Grid.RowDefinitions>
            <slabbcontrols:MenuBar Grid.Row="0"
                                   Grid.ColumnSpan="2"
                                   Margin="0,0,0,0"
                                   HorizontalAlignment="Right"
                                   Style="{StaticResource ctaBlueButtonBar}">
                <slabbcontrols:MenuItem Caption="New Search"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Click="btnNewSearch_Click"
                                        Margin="0,5,0,5"
                                        x:Name="btnNewSearch" />
                <slabbcontrols:MenuItem Caption="Form Preview"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        Click="btnFormPreview_Click"
                                        x:Name="btnFormPreview" />
                <slabbcontrols:MenuItem Caption="No Records Report"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        Click="btnNoRecords_Click"
                                        x:Name="btnNoRecords" />
                <slabbcontrols:MenuItem Caption="Perform Search($)"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        x:Name="btnPerformSearch"
                                        Click="btnPerformSearch_Click" />
                <slabbcontrols:MenuItem Caption="Submit"
                                        Cursor="Hand"
                                        Click="btnSubmit_Click"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        x:Name="btnSubmit" />
            </slabbcontrols:MenuBar>
            <Border Grid.Row="1"
                    Margin="0,0,0,20">
                <StackPanel Orientation="Vertical">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Order #"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.OrderNo}" />
                        <TextBlock Text="-"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.OrderItemNo}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Tracking #"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.TrackingNo}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="County Name"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.CountyName}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Searched On"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.SearchKey}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="Currency Date"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.CurrencyDate}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal"
                                Visibility="{Binding Path=HeaderInfo.FromDate,Converter={StaticResource EmptyStringToVisibility}}">
                        <TextBlock Text="Searched From"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.FromDate}" />
                        <TextBlock Text=" To " />
                        <TextBlock Text="{Binding Path=HeaderInfo.ToDate}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal"
                                Visibility="{Binding Path=HeaderInfo.LienType,Converter={StaticResource EmptyStringToVisibility}}">
                        <TextBlock Text="Searched Lien Types"
                                   Width="150" />
                        <TextBlock Text=":"
                                   Margin="5,0,5,0"></TextBlock>
                        <TextBlock Text="{Binding Path=HeaderInfo.LienType}" />
                    </StackPanel>
                </StackPanel>
            </Border>
            <TextBlock x:Name="lblMoreRecordsAvailable"
                       Text="County website's summary searches has data exceeding the upper cap [100 Records]."
                       Foreground="Red"
                       Grid.Row="3"
                       Visibility="Collapsed" />
            <tk:BusyIndicator x:Name="busyIndicator"
                              Grid.Row="3"
                              BusyContent="Please Wait While Submittng Details..."></tk:BusyIndicator>
            <TextBlock x:Name="lblSearchedOldTab"
                       Text="The 1871 thru 1946 search was not performed."
                       Foreground="Red"
                       Grid.Row="4"
                       Visibility="Collapsed" />
            <slabbrefreshable:RefreshIndicator x:Name="refreshIndicator"
                                               Grid.Row="5"
                                               Status="{Binding Status,ElementName=NameList, Mode=TwoWay}">
                <uxcontrols:MultiSelectGridView x:Name="grvAcceptedOrders"
                                                ItemsSource="{Binding Path=NameList}"
                                                LoadingRow="grvAcceptedOrders_LoadingRow"
                                                Style="{StaticResource uxcontrolsGridView}"
                                                CanUserSortColumns="False"
                                                EmptyGridMessage="Requested Party not on file.">
                    <uxcontrols:MultiSelectGridView.Columns>
                        <data:DataGridTemplateColumn Header="NAME"
                                                     SortMemberPath="PartyName"
                                                     Width="*">
                            <data:DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <uxcontrols:TruncatableTextBlock Margin="7"
                                                                     Text="{Binding PartyName}" />
                                </DataTemplate>
                            </data:DataGridTemplateColumn.CellTemplate>
                        </data:DataGridTemplateColumn>
                    </uxcontrols:MultiSelectGridView.Columns>
                </uxcontrols:MultiSelectGridView>
            </slabbrefreshable:RefreshIndicator>
            <slabbcontrols:MenuBar Grid.Row="6"
                                   Grid.ColumnSpan="2"
                                   Margin="0,0,0,0"
                                   HorizontalAlignment="Right"
                                   Style="{StaticResource ctaBlueButtonBar}">
                <slabbcontrols:MenuItem Caption="New Search"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        Click="btnNewSearch_Click"
                                        x:Name="btnNewSearchbtm" />
                <slabbcontrols:MenuItem Caption="Form Preview"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        Click="btnFormPreview_Click"
                                        x:Name="btnFormPreviewbtm" />
                <slabbcontrols:MenuItem Caption="No Records Report"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        Click="btnNoRecords_Click"
                                        x:Name="btnNoRecordsbtm" />
                <slabbcontrols:MenuItem Caption="Perform Search($)"
                                        Cursor="Hand"
                                        IsTabStop="True"
                                        Click="btnPerformSearch_Click"
                                        Margin="0,5,0,5"
                                        x:Name="btnPerformSearchbtm" />
                <slabbcontrols:MenuItem Caption="Submit"
                                        Cursor="Hand"
                                        Click="btnSubmit_Click"
                                        IsTabStop="True"
                                        Margin="0,5,0,5"
                                        x:Name="btnSubmitbtm" />
            </slabbcontrols:MenuBar>
        </Grid>
    </Grid>
</UserControl>
