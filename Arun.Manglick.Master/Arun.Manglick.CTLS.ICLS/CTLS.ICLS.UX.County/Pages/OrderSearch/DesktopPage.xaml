<shell:CPPage x:Class="CTLS.ICLS.UX.CountySearch.DesktopPage"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:shell="clr-namespace:CTLS.ICLS.UX.Controls;assembly=CTLS.ICLS.UX.Shell"
              xmlns:slabbcontrols="clr-namespace:CT.SLABB.UX.Controls;assembly=CT.SLABB.Controls"
              xmlns:local="clr-namespace:CTLS.ICLS.UX.CountySearch"              
              xmlns:slabbcorecontrols="clr-namespace:CT.SLABB.UX.Controls;assembly=CT.SLABB.Core">
    <Grid x:Name="LayoutRoot"
          Background="White">
        <local:CountySearchViewModel IsNew="True"/>
        <slabbcorecontrols:UxViewFrame x:Name="DesktopFrame"
                                       DefaultView="DesktopView"
                                       HorizontalContentAlignment="Stretch"
                                       Width="1003"
                                       BorderThickness="0"                                       
                                       VerticalContentAlignment="Stretch"
                                       Padding="10"/>
    </Grid>
</shell:CPPage>
